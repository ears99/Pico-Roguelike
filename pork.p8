--draws
function draw_game()
	cls(0)
	map()

	--dead mobs
	for m in all(dmob) do
		--make mobs flash on death
		if sin(time()*8) > 0 then
		draw_mob(m)
		m.dur -= 1
		end
		if m.dur <= 0 then
			del(dmob,m)
		end
	end

	--reguar mobs
	for i=#mob,1,-1 do
		draw_mob(mob[i])
	end

	for x=0,15 do
		for y=0,15 do
			if fog[x][y]==1 then
				drawrect(x*8,y*8,8,8,0)
			end
		end
	end

	for f in all(float) do
		oprint8(f.txt,f.x,f.y,f.c,0)
	end
end

function draw_mob(m)
	local col=10
	if m.flash>0 then
		m.flash-=1
		col=7
	end
	drawspr(getframe(m.ani),m.x*8+m.ox,m.y*8+m.oy,col,m.flp)
end

function draw_gameover()
	cls()
	print("you died", 35, 50, 7)
	print("press ❎ to restart",35,60,7)
end
