--main
function _init()
	t=0
	dpal={0,1,1,2,1,13,6,4,4,9,3,13,1,13,14}
	p_ani={240,241,242,243}
	--directions
	dirx={-1,1,0,0, 1,1,-1,-1}
	diry={0,0,-1,1,-1,1, 1,-1}

	--mob animations
	mob_ani={240,192}
	--mob attributes
	mob_atk={1,1} --attack power
	mob_hp={5,2} --hit points
	mob_los={4,4} --perception
	_upd=update_game
	_drw=draw_game

	--debugging system
	debug={}

	startgame()
end

function _update60()
	t+=1
	_upd()
	do_float()
	update_hp_wind()
end

function _draw()
	_drw()
	draw_wind()

	check_fade()
	--debugging system
	cursor(4,4)
	color(8)
	for txt in all(debug) do
		print(txt)
	end
end

function startgame()
	--start the game faded out
	fadeperc=1
--button buffer
	buttbuff=-1

--mob array
	mob={}
	--dead mob array
	dmob={}
	--player
	p_mob = add_mob(1,1,1)
	--enemies

	for x=1,15 do
		for y=1,15 do
			if mget(x,y)==3 then
				add_mob(2,x,y)
				mset(x,y,3)
			end
		end
	end

	p_t=0

	inv,eqp={},{}
	--eqp[1] - weapon
	--eqp[2] - armor
	--eqp[1-6] - inventory

--window/ui
	wind={}
	float={}
	talkwind=nil

	--fog of war
	--fill the map with fog: 1(default)
	--show the entire map: 0
	fog=blank_map(1) 
	
	
	
	hp_wind=addwind(5,5,28,13, {})

	_upd=update_game
	_drw=draw_game
	unfog()
	calc_dist(p_mob.x, p_mob.y)
end
