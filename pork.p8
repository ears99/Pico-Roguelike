--mob animations--
------------------
function ani_walk(mb)
	local tme=1-p_t
	mb.ox=mb.sox*tme
	mb.oy=mb.soy*tme
end

function ani_bump(mb)
	local tme=p_t
	if p_t>0.5 then
		tme=1-p_t
	end
	mb.ox=mb.sox*tme
	mb.oy=mb.soy*tme
end


